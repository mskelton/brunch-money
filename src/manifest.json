{
  "name": "Brunch Money",
  "description": "Helpers for Lunch Money",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon@16.png",
    "32": "images/icon@32.png",
    "48": "images/icon@48.png",
    "128": "images/icon@128.png",
    "256": "images/icon@256.png",
    "512": "images/icon@512.png"
  },
  "permissions": [],
  "host_permissions": [
    "https://my.lunchmoney.app/*",
    "https://beta.lunchmoney.app/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://my.lunchmoney.app/*",
        "https://beta.lunchmoney.app/*"
      ],
      "css": ["styles.css"],
      "js": ["content-script.js"]
    }
  ]
}
